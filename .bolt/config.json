{
  "template": "bolt-vite-react-ts",
  "build": {
    "command": "npm run build",
    "output": "dist",
    "nodeVersion": "18"
  },
  "dev": {
    "command": "npm run dev",
    "port": 5173
  },
  "install": {
    "command": "npm install"
  },
  "framework": "vite",
  "buildCache": {
    "paths": [
      "node_modules/.vite",
      ".vite"
    ]
  }
}
